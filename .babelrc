{
  "presets": [
    [
      "@babel/preset-env",
      {
        "forceAllTransforms": true,
        "modules": false,
        "useBuiltIns": false,
        "loose": true
      }
    ],
    "@babel/preset-react"
  ],

  "plugins": [
    [
      "@babel/plugin-transform-runtime", {
        "corejs": false,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ],
    "@babel/plugin-syntax-dynamic-import"
  ]
}